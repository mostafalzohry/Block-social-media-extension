{
    "name": "social media blocker",
    "description": "block facebook , twitter , instagram and youtube",
    "version": "1.0",
    "manifest_version": 3,
   
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
   "icons": { 
       "16": "icon16.png",
    "32": "icon32.png",
   "128": "icon128.png" }
}


 
  
